{
	"name": "daolabs-docs",
	"version": "2.1.0",
	"license": "LICENSE.md",
	"private": true,
	"lint-staged": {
		"*.{ts,svelte}": "yarn run lint:fix",
		"*.{html,json}": "prettier --write"
	},
	"scripts": {
		"dev": "yarn prepare && vite dev",
		"build": "yarn prepare && vite build && yarn ipfs-404",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint --cache \"src/**/*.{ts,svelte}\"",
		"lint:fix": "eslint --cache --fix",
		"format": "prettier --write .",
		"prepare": "sh scripts/prepare.sh && husky install .husky",
		"ipfs-404": "./scripts/ipfs-404.sh",
		"precommit": "pretty-quick --staged"
	},
	"devDependencies": {
		"@babel/core": "^7.19.3",
		"@commitlint/cli": "^17.3.0",
		"@commitlint/config-conventional": "^17.3.0",
		"@playwright/test": "^1.25.0",
		"@sveltejs/adapter-static": "^1.0.0-next.39",
		"@sveltejs/kit": "^1.0.0-next.431",
		"@types/github-slugger": "^1.3.0",
		"@types/node": "^18.11.9",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"auto-changelog": "^2.4.0",
		"autoprefixer": "^10.4.12",
		"babel-plugin-macros": "^3.1.0",
		"docx": "^7.7.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.1",
		"mdsvex": "^0.10.6",
		"postcss-load-config": "^4.0.1",
		"prettier": "2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"pretty-quick": "^3.1.3",
		"standard-version": "^9.5.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0-beta.1"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.1.0",
		"@types/file-saver": "^2.0.5",
		"@types/marked": "^4.0.7",
		"@web3-onboard/core": "^2.12.1",
		"@web3-onboard/injected-wallets": "^2.4.0",
		"@web3-onboard/ledger": "^2.3.2",
		"@web3-onboard/trezor": "^2.3.2",
		"@web3-onboard/walletconnect": "^2.2.2",
		"file-saver": "^2.0.5",
		"firebase": "^9.15.0",
		"github-slugger": "^1.0.0",
		"html-docx-js-typescript": "^0.1.5",
		"html-to-docx": "^1.4.0",
		"lodash": "^4.17.21",
		"marked": "^4.3.0",
		"marked-admonition-extension": "^0.0.1",
		"marked-extended-tables": "^1.0.5",
		"md-to-pdf": "^5.1.0",
		"mdast-util-to-string": "^3.1.0",
		"postcss": "^8.4.19",
		"rehype-autolink-headings": "^6.1.1",
